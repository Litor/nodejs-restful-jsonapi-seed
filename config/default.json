{
  "cors": {
    "origin": "*",
    "methods": ["DELETE", "GET", "PATCH", "POST", "PUT"],
    "credentials": false
  },
  "router": {
    "prefix": "/",
    "caseSensitive": false,
    "strictRouting": false,
    "accessControl": {
      "excludePaths": [
        "/ap[ip]-doc.*",
        "/login",
        "/wildcard-example/.*"
      ],
      "grantsObject": {
        "admin": {
          "/user": {
            "create:any": ["*"],
            "read:any": ["*"],
            "update:any": ["*"],
            "delete:any": ["*"]
          }
        }
      }
    }
  },
  "server": {
    "name": "0.0.0.0",
    "port": 3000,
    "parser": {
      "bodyLimit": 102400
    },
    "uploads": {
      "abortOnLimit": true,
      "fileLimit": 5242880,
      "safeFileNames": true
    },
    "allowEmptyBody": false,
    "xPoweredBy": ""
  },
  "session": {
    "name": "ssid",
    "secret": "TEMPORARY SECRET",
    "resave": false,
    "cookie": {
      "secure": false
    }
  }
}
