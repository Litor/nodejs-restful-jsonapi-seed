{
  "cors": {
    "origin": "*",
    "methods": ["DELETE", "GET", "PATCH", "POST", "PUT"],
    "credentials": "false"
  },
  "router": {
    "prefix": "/",
    "accessControl": {
      "excludePaths": ["/user"],
      "grantsObject": {
        "admin": {
          "user": {
            "create:any": ["*"],
            "read:any": ["*"],
            "update:any": ["*"],
            "delete:any": ["*"]
          }
        }
      }
    }
  },
  "server": {
    "port": "3000",
    "parser": {
      "bodyLimit": "102400"
    },
    "uploads": {
      "abortOnLimit": "true",
      "fileLimit": "5242880",
      "safeFileNames": "true"
    }
  }
}
